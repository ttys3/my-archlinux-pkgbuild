  'ca9c67390e763f0a17fdd13a1531424dcccc926c726ddfb84b25b4c39b48cb02'
)
install=jbr-linux-x64.install

package() {
  install -dm755 "${pkgdir}/opt"
  cp -a "${srcdir}/jbr" "${pkgdir}/opt/"
}

